{
  "name": "tailwind-nextjs-starter-blog",
  "version": "2.1.0",
  "private": true,
  "scripts": {
    "start": "next dev",
    "prepare": "node ./scripts/prebuild.mjs && contentlayer2 build",
    "dev": "cross-env INIT_CWD=$PWD next dev",
    "build": "cross-env INIT_CWD=$PWD next build",
    "deploy": "npm run prepare && npmrun build",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir app --dir components --dir lib --dir layouts --dir scripts"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.2.2",
    "@next/bundle-analyzer": ">=14.2.18",
    "@nextui-org/react": "^2.2.10",
    "@portaljs/remark-wiki-link": "^1.1.2",
    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/typography": "^0.5.9",
    "@types/lodash": "^4.14.202",
    "@types/moment": "^2.13.0",
    "@types/pluralize": "^0.0.33",
    "@types/unist": "^3.0.2",
    "autoprefixer": "^10.4.13",
    "chalk": "^5.3.0",
    "contentlayer2": "^0.5.3",
    "dotenv": "^16.4.5",
    "esbuild": "0.24.0",
    "framer-motion": "^11.0.15",
    "fs-extra": "^11.2.0",
    "github-slugger": "^2.0.0",
    "glob": "^11.0.0",
    "gray-matter": "^4.0.2",
    "image-size": "1.1.1",
    "lodash": "^4.17.21",
    "mdast-util-from-markdown": "^2.0.0",
    "mdast-util-wiki-link": "^0.1.2",
    "micromark": "^4.0.0",
    "micromark-extension-wiki-link": "^0.0.4",
    "moment": "^2.30.1",
    "next": ">=14.2.18",
    "next-contentlayer2": "^0.5.3",
    "pliny": "^0.4.0",
    "pluralize": "^8.0.0",
    "postcss": "^8.4.24",
    "probe-image-size": "^7.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hover-video-player": "^10.0.1",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-citation": "^2.2.2",
    "rehype-katex": "^7.0.0",
    "rehype-preset-minify": "7.0.1",
    "rehype-prism-plus": "^2.0.0",
    "rehype-slug": "^6.0.0",
    "remark": "^15.0.0",
    "remark-gfm": "^4.0.0",
    "remark-github-blockquote-alert": "^1.2.1",
    "remark-math": "^6.0.0",
    "sass": "^1.71.1",
    "sharp": "^0.33.4",
    "swiper": "^11.0.7",
    "tailwindcss": "^3.4.1",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.0.1",
    "@types/mdx": "^2.0.5",
    "@types/node": "22.9.0",
    "@types/react": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.14.0",
    "eslint-config-next": "15.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "typescript": "^5.1.3"
  },
  "resolutions": {
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/core": "1.27.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "0.54.2",
    "@opentelemetry/resources": "1.27.0",
    "@opentelemetry/sdk-trace-base": "1.27.0",
    "@opentelemetry/sdk-trace-node": "1.27.0",
    "@opentelemetry/semantic-conventions": "1.27.0"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
