{
  "name": "tailwind-nextjs-starter-blog",
  "version": "2.1.0",
  "private": true,
  "scripts": {
    "start": "next dev",
    "prebuild": "node ./scripts/prebuild.mjs",
    "dev": "node ./scripts/prebuild.mjs && cross-env INIT_CWD=$PWD next dev",
    "build": "node ./scripts/prebuild.mjs && cross-env INIT_CWD=$PWD next build && cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/postbuild.mjs",
    "serve": "next start",
    "fastbuild": "cross-env INIT_CWD=$PWD next build",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir app --dir components --dir lib --dir layouts --dir scripts"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.2.2",
    "@next/bundle-analyzer": "14.1.0",
    "@nextui-org/react": "^2.2.10",
    "@portaljs/remark-wiki-link": "^1.1.2",
    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/typography": "^0.5.9",
    "@types/lodash": "^4.14.202",
    "@types/moment": "^2.13.0",
    "@types/pluralize": "^0.0.33",
    "@types/unist": "^3.0.2",
    "autoprefixer": "^10.4.13",
    "chalk": "^5.3.0",
    "contentlayer": "0.3.4",
    "esbuild": "0.18.11",
    "framer-motion": "^11.0.15",
    "fs-extra": "^11.2.0",
    "github-slugger": "^2.0.0",
    "glob": "^10.3.10",
    "gray-matter": "^4.0.2",
    "image-size": "1.0.0",
    "lodash": "^4.17.21",
    "mdast-util-from-markdown": "^2.0.0",
    "mdast-util-wiki-link": "^0.1.2",
    "micromark": "^4.0.0",
    "micromark-extension-wiki-link": "^0.0.4",
    "moment": "^2.30.1",
    "next": "14.2.3",
    "next-contentlayer": "0.3.4",
    "pliny": "0.1.7",
    "pluralize": "^8.0.0",
    "postcss": "^8.4.24",
    "probe-image-size": "^7.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hover-video-player": "^10.0.1",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^6.1.0",
    "rehype-citation": "^1.0.2",
    "rehype-katex": "^6.0.3",
    "rehype-preset-minify": "6.0.0",
    "rehype-prism-plus": "^1.6.0",
    "rehype-slug": "^5.1.0",
    "remark": "^14.0.2",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "sass": "^1.71.1",
    "swiper": "^11.0.7",
    "tailwindcss": "^3.4.1",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.0.1",
    "@types/mdx": "^2.0.5",
    "@types/node": "20.14.2",
    "@types/react": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.45.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.0",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "typescript": "^5.1.3"
  },
  "resolutions": {
    "@opentelemetry/api": "1.4.1",
    "@opentelemetry/core": "1.13.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "0.39.1",
    "@opentelemetry/resources": "1.13.0",
    "@opentelemetry/sdk-trace-base": "1.13.0",
    "@opentelemetry/sdk-trace-node": "1.13.0",
    "@opentelemetry/semantic-conventions": "1.13.0"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
